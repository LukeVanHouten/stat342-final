library(tidyverse)
library(maxLik)
library(reshape2)
library(fastR2)
# Chunk 2
loglik <- function(psi, T, n){
if (psi > 1 | psi < 0)
return(NA)
else
return(log(choose(n, T)) + (T * log(1 - psi)) - (n * log(2 - psi)))
}
estimation <- maxLik2(loglik = loglik, start = 0.55, method = "NR", tol = 1e-4,
T = 8, n = 170)
print(estimation)
